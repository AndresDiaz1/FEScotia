{
  "name": "FEScotia",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@capacitor/core": "2.4.5",
    "@fortawesome/fontawesome-free": "5.15.0",
    "@fortawesome/fontawesome-svg-core": "1.2.30",
    "@fortawesome/free-regular-svg-icons": "5.14.0",
    "@fortawesome/free-solid-svg-icons": "5.14.0",
    "@fortawesome/react-fontawesome": "0.1.11",
    "@ionic/react": "^5.0.7",
    "@ionic/react-router": "^5.0.7",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.3",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.24",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-mock-store": "1.0.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "fp-ts": "^2.9.1",
    "ionicons": "^5.0.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "node-sass": "4.14.1",
    "prettier": "^2.0.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "typescript": "3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --ci --coverage --reporters=\"default\" --reporters=\"jest-junit\" --no-watchman --watchAll --watchAll=false",
    "eject": "react-scripts eject",
    "format": "prettier \"src/**/*.{ts,tsx,js,html}\" --write",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --quiet --ignore-pattern **/*.scss --ignore-pattern **/*.test.* --ignore-pattern **/*__tests__*  --ignore-pattern **/serviceWorker.ts --ignore-pattern /src/vendor/*",
    "lint:fix": "npm run lint --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "2.4.5",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "24.9.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.3",
    "enzyme-to-json": "3.5.0",
    "jest-junit": "10.0.0",
    "redux-mock-store": "1.5.4"
  },
  "description": "An Ionic project",
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!@ionic|ionicons|@capacitor-community/react-hooks|@fortawesome|capacitor-data-storage-sqlite)"
    ],
    "coveragePathIgnorePatterns": [
      "src/serviceWorker.ts",
      "src/index.tsx"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
